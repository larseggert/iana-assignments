<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="cxtp-parameters.xsl"?>
<?xml-model href="cxtp-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="cxtp-parameters">
  <title>Context Transfer Protocol (CXTP) Parameters</title>
  <created>2005-01-19</created>
  <updated>2023-02-21</updated>
  <registry id="cxtp-parameters-1">
    <title>Context Transfer Profile Type Registry</title>
    <xref type="rfc" data="rfc4065"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <code>0x00</code>
      <profile>Reserved</profile>
      <xref type="rfc" data="rfc4065"/>
    </record>
    <record>
      <code>0x01</code>
      <profile>IPv6 Multicast Listener Context</profile>
      <xref type="rfc" data="rfc4065"/>
    </record>
  </registry>
  <registry id="cxtp-parameters-2">
    <title>Context Transfer Protocol Authorization Token Calculation Algorithms</title>
    <xref type="rfc" data="rfc4065"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <code>0x1</code>
      <algorithm>HMAC_SHA1</algorithm>
      <xref type="rfc" data="rfc4065"/>
    </record>
  </registry>
  <people/>
</registry>

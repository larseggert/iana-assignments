<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="gsmpv3-traffic.xsl"?>
<?xml-model href="gsmpv3-traffic.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="gsmpv3-traffic">
  <title>Global Switch Management Protocol version 3 (GSMPv3) Traffic Control Name Space</title>
  <created>2002-03-28</created>
  <updated>2023-08-25</updated>
  <registry id="gsmpv3-traffic-1">
    <title>Traffic Control Flags</title>
    <xref type="rfc" data="rfc3292"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Stefaan De Cnodder, Avri Doria</expert>
    <record>
      <value>0b00</value>
      <description>Indicates that the Traffic Control is not   
available in the Capability Set.</description>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <value>0b01</value>
      <description>Indicates that the Traffic Control is       
applied to all connections that use the
Capability Set.</description>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <value>0b10</value>
      <description>Indicates that the Traffic Control is       
available for application to connections
that use the Capability Set on a per
connection basis.</description>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <value>0b11</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc3292"/>
    </record>
  </registry>
  <people/>
</registry>

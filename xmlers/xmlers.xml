<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="xmlers.xsl"?>
<?oxygen RNGSchema="xmlers.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="xmlers">
  <title>XML Evidence Record Syntax (XMLERS)</title>
  <created>2011-04-13</created>
  <updated>2011-07-20</updated>
  <registry id="time-stamp-token">
    <title>Time-Stamp Token Type</title>
    <xref type="rfc" data="rfc6283"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <value>RFC3161</value>
      <description>RFC3161 Time-Stamp</description>
      <xref type="rfc" data="rfc3161"/>
    </record>
    <record>
      <value>XMLENTRUST</value>
      <description>EnTrust XML Schema</description>
      <xref type="uri" data="http://www.si-tsa.gov.si/dokumenti/timestamp-protocol-20020207.xsd"/>
    </record>
  </registry>
  
  <registry id="crypto-info">
    <title>Cryptographic Information Type</title>
    <xref type="rfc" data="rfc6283"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <value>CERT</value>
      <description>DER-encoded X.509 Certificate</description>
      <xref type="rfc" data="rfc5280"/>
    </record>
    <record>
      <value>CRL</value>
      <description>DER-encoded X.509 Certificate Revocation List</description>
      <xref type="rfc" data="rfc5280"/>
    </record>
    <record>
      <value>OCSP</value>
      <description>DER-encoded OCSPResponse</description>
      <xref type="rfc" data="rfc2560"/>
    </record>
    <record>
      <value>SCVP</value>
      <description>DER-encoded SCVP response (CVResponse)</description>
      <xref type="rfc" data="rfc5055"/>
    </record>
  </registry>
  
  <people/>
</registry>

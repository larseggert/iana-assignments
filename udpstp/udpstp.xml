    <xref type="draft" data="RFC-ietf-ippm-capacity-protocol-25"/>
    <range>
    <value>0x0000</value>
    <registration_rule>Reserved</registration_rule>
    </range>
    <range>
    <value>0x0001-0x7F00</value>
    <registration_rule>Specification Required</registration_rule>
    </range>

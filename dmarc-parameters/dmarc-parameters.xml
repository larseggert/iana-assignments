<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="dmarc-parameters.xsl"?>
<?xml-model href="dmarc-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="dmarc-parameters">
  <title>Domain-based Message Authentication, Reporting, and Conformance (DMARC) Parameters</title>
  <created>2015-01-27</created>
  <updated>2021-07-27</updated>
  
  <registry id="tag">
    <title>DMARC Tag Registry</title>
    <xref type="rfc" data="rfc7489"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Scott Kitterman</expert>
    <record date="2015-01-27">
      <name>adkim</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>DKIM alignment mode</description>
    </record>
    <record date="2015-01-27">
      <name>aspf</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>SPF alignment mode</description>
    </record>
    <record date="2015-01-27">
      <name>fo</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Failure reporting options</description>
    </record>
    <record date="2021-05-26">
      <name>np</name>
      <xref type="rfc" data="rfc9091"/>
      <status>current</status>
      <description>Requested handling policy for non-existent subdomains</description>
    </record>
    <record date="2015-01-27">
      <name>p</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Requested handling policy</description>
    </record>
    <record date="2015-01-27">
      <name>pct</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Sampling rate</description>
    </record>   
    <record date="2015-01-27">
      <name>rf</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Failure reporting format(s)</description>
    </record>
    <record date="2015-01-27">
      <name>ri</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Aggregate Reporting interval</description>
    </record>
    <record date="2015-01-27">
      <name>rua</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Reporting URI(s) for aggregate data</description>
    </record>
    <record date="2015-01-27">
      <name>ruf</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Reporting URI(s) for failure data</description>
    </record>
    <record date="2015-01-27">
      <name>sp</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Requested handling policy for subdomains</description>
    </record>
    <record date="2015-01-27">
      <name>v</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Specification version</description>
    </record>    
  </registry>
  
  <registry id="report-format">
    <title>DMARC Report Format Registry</title>
    <xref type="rfc" data="rfc7489"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Scott Kitterman</expert>
    <record>
      <name>afrf</name>
      <xref type="rfc" data="rfc7489"/>
      <status>current</status>
      <description>Authentication Failure Reporting Format 
        (see <xref type="rfc" data="rfc6591"/>)</description>
    </record>
  </registry>
  
  
  <people/>
</registry>

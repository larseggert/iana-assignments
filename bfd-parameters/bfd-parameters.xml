<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="bfd-parameters.xsl"?>
<?xml-model href="bfd-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="bfd-parameters">
  <title>Bidirectional Forwarding Detection (BFD) Parameters</title>
  <created>2010-03-29</created>
  <updated>2021-10-25</updated>
  
  <registry id="bfd-parameters-1">
    <title>BFD Diagnostic Codes</title>
    <xref type="rfc" data="rfc5880"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Dave Ward (backup experts: Dave Katz, Jeff Haas, Les Ginsberg)</expert>
    <note>Updates to this registry must be mirrored in YANG module 
<xref type="registry" data="iana-bfd-types">iana-bfd-types</xref>. See <xref type="rfc" data="rfc9127"/>. 
    </note>
    <record>
      <code>0</code>
      <profile>No Diagnostic</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>1</code>
      <profile>Control Detection Time Expired</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>2</code>
      <profile>Echo Function Failed</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>3</code>
      <profile>Neighbor Signaled Session Down</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>4</code>
      <profile>Forwarding Plane Reset</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>5</code>
      <profile>Path Down</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>6</code>
      <profile>Concatenated Path Down</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>7</code>
      <profile>Administratively Down</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>8</code>
      <profile>Reverse Concatenated Path Down</profile>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>9</code>
      <profile>mis-connectivity defect</profile>
      <xref type="rfc" data="rfc6428"/>
    </record>
    <record>
      <code>10-31</code>
      <profile>Unassigned</profile>
    </record>

  </registry>
  <registry id="bfd-parameters-2">
    <title>BFD Authentication Types</title>
    <xref type="rfc" data="rfc5880"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Dave Ward (backup experts: Dave Katz, Jeff Haas, Les Ginsberg)</expert>
    <note>Updates to this registry must be mirrored in YANG module 
<xref type="registry" data="iana-bfd-types">iana-bfd-types</xref>. See <xref type="rfc" data="rfc9127"/>. 
    </note>
    <record>
      <code>0</code>
      <algorithm>Reserved</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>1</code>
      <algorithm>Simple Password</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>2</code>
      <algorithm>Keyed MD5</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>3</code>
      <algorithm>Meticulous Keyed MD5</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>4</code>
      <algorithm>Keyed SHA1</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>5</code>
      <algorithm>Meticulous Keyed SHA1</algorithm>
      <xref type="rfc" data="rfc5880"/>
    </record>
    <record>
      <code>6-255</code>
      <algorithm>Unassigned</algorithm>
    </record> 
  </registry>
  <people/>
</registry>

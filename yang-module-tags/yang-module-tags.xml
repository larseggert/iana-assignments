<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="yang-module-tags.xsl"?>
<?xml-model href="yang-module-tags.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="yang-module-tags">
  <title>YANG Module Tags</title>
  <created>2020-03-23</created>
  <updated>2021-02-05</updated>

  <registry id="yang-module-tag-prefixes">
    <title>YANG Module Tag Prefixes</title>
    <registration_rule>Specification Required</registration_rule>
    <expert>Christian Hopps, Lou Berger, Dean Bogdanovic</expert>
    <xref type="rfc" data="rfc8819"/>
    <record date="2020-03-23">
      <value>ietf:</value>
      <description>IETF tags allocated in the IANA "IETF YANG Module Tags‚Äù registry.</description>
      <assignee>IETF</assignee>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>vendor:</value>
      <description>Non-registered tags allocated by the module implementer.</description>
      <assignee>IETF</assignee>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>user:</value>
      <description>Non-registered tags allocated by and for the user.</description>
      <assignee>IETF</assignee>
      <xref type="rfc" data="rfc8819"/>
    </record>
  </registry>
  
  <registry id="ietf-yang-module-tags">
    <title>IETF YANG Module Tags</title>
    <registration_rule>IETF Review</registration_rule>
    <xref type="rfc" data="rfc8819"/>
    <record date="2020-03-23">
      <value>ietf:network-element-class</value>
      <description>Network element as defined in <xref type="rfc" data="rfc8199"/>.</description>
      <xref type="rfc" data="rfc8199"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:network-service-class</value>
      <description>Network service as defined in <xref type="rfc" data="rfc8199"/>.</description>
      <xref type="rfc" data="rfc8199"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:sdo-defined-class</value>
      <description>Module is defined by a standards organization.</description>
      <xref type="rfc" data="rfc8199"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:vendor-defined-class</value>
      <description>Module is defined by a vendor.</description>
      <xref type="rfc" data="rfc8199"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:user-defined-class</value>
      <description>Module is defined by the user.</description>
      <xref type="rfc" data="rfc8199"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:hardware</value>
      <description>Relates to hardware (e.g., inventory).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:software</value>
      <description>Relates to software (e.g., installed OS).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:protocol</value>
      <description>Represents a protocol (often combined with another tag to refine).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:qos</value>
      <description>Relates to quality of service.</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:network-service-app</value>
      <description>Relates to a network service application (e.g., an NTP server, 
        DNS server, DHCP server, etc).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:system-management</value>
      <description>Relates to system management (e.g., a system management protocol 
      such as syslog, TACAC+, SNMP, netconf, etc).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:oam</value>
      <description>Relates to Operations, Administration, and Maintenance (e.g., BFD).</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:routing</value>
      <description>Relates to routing.</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:security</value>
      <description>Relates to security.</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:signaling</value>
      <description>Relates to control-plane signaling.</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
    <record date="2020-03-23">
      <value>ietf:link-management</value>
      <description>Relates to link management.</description>
      <xref type="rfc" data="rfc8819"/>
    </record>
  </registry>
  
  
  <people/>

</registry>

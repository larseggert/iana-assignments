<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="olsr-types.xsl"?>
<?xml-model href="olsr-types.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="olsr-types">
  <title>Optimized Link State Routing (OLSR) Protocol Message Types</title>
  <created>2003-08-28</created>
  <updated>2003-10-31</updated>
  <registry id="olsr-types-2">
    <title>OLSR Protocol Message Types</title>
    <xref type="rfc" data="rfc3626"/>
    <range>
      <value>1-127</value>
      <registration_rule>Standards Action</registration_rule>
    </range>
    <range>
      <value>128-255</value>
      <registration_rule>Private/Local Use</registration_rule>
      <note>IANA does not assign</note>
    </range>
    <record>
      <value>1</value>
      <description>HELLO_MESSAGE</description>
      <xref type="rfc" data="rfc3626"/>
    </record>
    <record>
      <value>2</value>
      <description>TC_MESSAGE</description>
      <xref type="rfc" data="rfc3626"/>
    </record>
    <record>
      <value>3</value>
      <description>MID_MESSAGE</description>
      <xref type="rfc" data="rfc3626"/>
    </record>
    <record>
      <value>4</value>
      <description>HNA_MESSAGE</description>
      <xref type="rfc" data="rfc3626"/>
    </record>
    <record>
      <value>5-127</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>128-255</value>
      <description>Reserved for Private/Local Use</description>
    </record>
  </registry>
  <people/>
</registry>

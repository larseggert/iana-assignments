<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="alert-urns.xsl"?>
<?xml-model href="alert-urns.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="alert-urns">
  <category>Uniform Resource Names (URN)</category>
  <title>Alert URNs</title>
  <created>2014-11-24</created>
  <updated>2015-03-25</updated>
  
  <registry id="identifiers">
    <title>Alert URN Identifiers</title>
    <xref type="rfc" data="rfc7462"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record date="2014-11-24">
      <value>service</value>
      <description>Specific telephony service used in this call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:normal</value>
      <description>Normal ring/ringback rendering (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:call-waiting</value>
      <description>Call waiting was initiated at the other side of the call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:forward</value>
      <description>Call has been forwarded</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:recall:callback</value>
      <description>Recall due to callback</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:recall:hold</value>
      <description>Recall due to call hold</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>service:recall:transfer</value>
      <description>Recall due to transfer</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source</value>
      <description>Classification of the other party to the call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source:unclassified</value>
      <description>Unclassified ring/ringback rendering (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source:internal</value>
      <description>User at the other side of the call is internal to the enterprise or PBX system</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source:external</value>
      <description>User at the other side of the call is external to the enterprise or PBX system</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source:friend</value>
      <description>User at the other side of the call is a friend</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>source:family</value>
      <description>User at the other side of the call is a family member</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>priority</value>
      <description>Priority of the call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>priority:normal</value>
      <description>Normal ring/ringback rendering (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>priority:low</value>
      <description>Low priority call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>priority:high</value>
      <description>High priority call</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24" updated="2014-12-05">
      <value>duration</value>
      <description>Duration of alerting signal</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>duration:normal</value>
      <description>Normal ring/ringback rendering (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>duration:short</value>
      <description>Shorter than normal</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>duration:long</value>
      <description>Longer than normal</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>delay</value>
      <description>Delay of rendering of alerting signal</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>delay:none</value>
      <description>Immediate alerting (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>delay:yes</value>
      <description>Delayed alerting</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>locale</value>
      <description>Location-specific alerting signals</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>locale:default</value>
      <description>Alerting not location-specific (default value)</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
    <record date="2014-11-24">
      <value>locale:country:&lt;ISO 3166-1 country code&gt;</value>
      <description>Alerting according to the conventions of the specified country</description>
      <xref type="rfc" data="rfc7462"/>
    </record>
  </registry>
  
  
  <registry id="providers">
    <title>Alert URN Providers</title>
    <xref type="rfc" data="rfc7462"/>
    <registration_rule>First Come First Served</registration_rule>
    
    <record date="2014-11-24">
      <value>example</value>
      <description>IETF</description>
      <contact>mailto:rai-ads&amp;tools.ietf.org</contact>
    </record>
  </registry>
  
  <people/>
</registry>

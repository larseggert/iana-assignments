<registry xmlns="http://www.iana.org/assignments" id="imap-mailbox-name-attributes">
  <category>Internet Message Access Protocol (IMAP) Capabilities</category>
  <title>IMAP Mailbox Name Attributes</title>
  <created>2018-06-14</created>
  <updated>2018-09-10</updated>

  <registry id="imap-mailbox-name-attributes-1">
    <title>IMAP Mailbox Name Attributes</title>
    <xref type="rfc" data="rfc3501">RFC3501, Section 7.2.2</xref>
    <xref type="rfc" data="rfc8457">RFC8457, Section 6</xref>
    <registration_rule>IETF Review or Expert Review</registration_rule>
    <expert>Barry Leiba, Bron Gondwana</expert>
    
    <record date="2018-06-14">
      <value>All</value>
      <description>All messages</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Archive</value>
      <description>Archived messages</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Drafts</value>
      <description>Messages that are working drafts</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Flagged</value>
      <description>Messages with the \Flagged flag</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>HasChildren</value>
      <description>Has accessible child mailboxes</description>
      <xref type="rfc" data="rfc5258"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>HasNoChildren</value>
      <description>Has no accessible child mailboxes</description>
      <xref type="rfc" data="rfc5258"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>Important</value>
      <description>Messages deemed important to user</description>
      <xref type="rfc" data="rfc8457"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Junk</value>
      <description>Messages identified as Spam/Junk</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Marked</value>
      <description>Server has marked the mailbox as "interesting"</description>
      <xref type="rfc" data="rfc3501"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>NoInferiors</value>
      <description>No hierarchy under this name</description>
      <xref type="rfc" data="rfc3501"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>NonExistent</value>
      <description>The mailbox name doesn't actually exist</description>
      <xref type="rfc" data="rfc5258"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>Noselect</value>
      <description>The mailbox is not selectable</description>
      <xref type="rfc" data="rfc3501"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>Remote</value>
      <description>The mailbox exists on a remote server</description>
      <xref type="rfc" data="rfc5258"/>
      <notes>not used by JMAP</notes>
    </record>
    <record date="2018-06-14">
      <value>Sent</value>
      <description>Sent mail</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Subscribed</value>
      <description>The mailbox is subscribed to</description>
      <xref type="rfc" data="rfc5258"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Trash</value>
      <description>Messages the user has discarded</description>
      <xref type="rfc" data="rfc6154"/>
      <notes/>
    </record>
    <record date="2018-06-14">
      <value>Unmarked</value>
      <description>No new messages since last select</description>
      <xref type="rfc" data="rfc3501"/>
      <notes>not used by JMAP</notes>
    </record>
    

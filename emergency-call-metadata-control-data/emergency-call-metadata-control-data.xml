  <registry id="emergency-call-vehicle-camera-id">
    <title>Emergency Call Vehicle Camera ID</title>
    <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
    <record date="2017-02-28">
      <value>backup</value>
      <description>Shows what is behind the vehicle, e.g., often used 
        for driver display when the vehicle is in reverse. 
        Also known as rearview, reverse, rear visibility, etc.</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>left-rear</value>
      <description>Shows view to the left and behind (e.g., left side 
        rear-view mirror or blind spot view)</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>right-rear</value>
      <description>Shows view to the right and behind (e.g., right 
        side rear-view mirror or blind spot view)</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>forward</value>
      <description>Shows what is in front of the vehicle</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>rear-wide</value>
      <description>Shows what is behind vehicle (e.g., used by rear-collision 
        detection systems), separate from backup view</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>lane</value>
      <description>Used by systems to identify road lane and/or 
        monitor vehicle's position within lane</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>interior</value>
      <description>Shows the interior (e.g., driver)</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>night-front</value>
      <description>Night-vision view of what is in front of the vehicle</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>night-rear</value>
      <description>Night-vision view of what is behind the vehicle</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>night-left</value>
      <description>Night-vision view of what is to the left of the vehicle</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>
    <record date="2017-02-28">
      <value>night-right</value>
      <description>Night-vision view of what is to the right of the vehicle</description>
      <xref type="draft" data="RFC-ietf-ecrit-car-crash-23"/>
      <controller>IESG</controller>
    </record>

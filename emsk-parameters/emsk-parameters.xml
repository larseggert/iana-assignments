<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="emsk-parameters.xsl"?>
<?xml-model href="emsk-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="emsk-parameters">
  <title>Extended Master Session Key (EMSK) Parameters</title>
  <created>2008-06-24</created>
  <updated>2024-06-21</updated>
  
  <registry id="emsk-parameters-1">
    <title>User Specific Root Keys (USRK) Key Labels</title>
    <xref type="rfc" data="rfc5295"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <label>experimental1</label>
      <description>Reserved for Experimental Use</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>experimental2</label>
      <description>Reserved for Experimental Use</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>private1</label>
      <description>Reserved for Private Use</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>private2</label>
      <description>Reserved for Private Use</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>EMSK</label>
      <description>Extended Master Session Key (EMSK)</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>dsrk@ietf.org</label>
      <description>Domain Specific Root Key (DSRK)</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <label>EAP Re-authentication Root Key@ietf.org</label>
      <description>Re-auth usage label</description>
      <xref type="rfc" data="rfc6696"/>
    </record>
    <record>
      <label>DSRK Delivery Authorized Key@ietf.org</label>
      <description>DSRK authorized delivery key</description>
      <xref type="rfc" data="rfc6696"/>
    </record>
    <record>
      <label>EAP Early-Authentication Root Key@ietf.org</label>
      <description>Early authentication usage</description>
      <xref type="rfc" data="rfc6630"/>
    </record>
    <record date="2014-01-24" updated="2024-06-21">
      <label>TEAPbindkey@ietf.org</label>
      <description>TEAP binding usage label</description>
      <xref type="draft" data="RFC-ietf-emu-rfc7170bis-19"/>
    </record>
  </registry>
  
  <registry id="emsk-parameters-3">
    <title>EMSK PRF Numbers</title>
    <xref type="rfc" data="rfc5295"/>
    <range>
      <value>0-220</value>
      <registration_rule>IETF Review</registration_rule>
    </range>
    <range>
      <value>221-255</value>
      <registration_rule>Reserved for Private Use</registration_rule>
    </range>
    <record>
      <value>0</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <value>1</value>
      <description>HMAC-SHA-256 PRF+ (Default)</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
    <record>
      <value>2-220</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>221-255</value>
      <description>Reserved for Private Use</description>
      <xref type="rfc" data="rfc5295"/>
    </record>
  </registry>
  <people/>
</registry>

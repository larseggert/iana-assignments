<registry xmlns="http://www.iana.org/assignments" id="tesla-parameters">
  <title>Timed Efficient Stream Loss-tolerant Authentication (TESLA) Parameters</title>
  <created>2006-02-14</created>
  <updated>2010-04-07</updated>
  <registry id="tesla-parameters-1">
    <title>Cryptographic Pseudo-Random Function, TESLA-PRF Values</title>
    <xref type="rfc" data="rfc4442"/>
    <registration_rule>Expert Review by the MSEC working group or its designated successor</registration_rule>
    <record>
      <value>0</value>
      <description>HMAC-SHA1</description>
      <xref type="rfc" data="rfc4442"/>
    </record>
    <record>
      <value>1</value>
      <description>HMAC-SHA-224</description>
      <xref type="rfc" data="rfc5776"/>
    </record>
    <record>
      <value>2</value>
      <description>HMAC-SHA-256 (default)</description>
      <xref type="rfc" data="rfc5776"/>
    </record>
    <record>
      <value>3</value>
      <description>HMAC-SHA-384</description>
      <xref type="rfc" data="rfc5776"/>
    </record>
    <record>
      <value>4</value>
      <description>HMAC-SHA-512</description>
      <xref type="rfc" data="rfc5776"/>
    </record>
    <record>
      <value>5-240</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>241-255</value>
      <description>Private Use</description>
      <xref type="rfc" data="rfc4442"/>
    </record>
  </registry>

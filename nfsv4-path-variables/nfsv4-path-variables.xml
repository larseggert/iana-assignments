<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="nfsv4-path-variables.xsl"?>
<?xml-model href="nfsv4-path-variables.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="nfsv4-path-variables">
  <title>NFSv4 Path Variables</title>
  <category>Network File System version 4 (NFSv4)</category>
  <created>2009-01-16</created>
  <updated>2023-02-21</updated>

  <registry id="nfsv4-path-variables-1">
    <title>NFSv4 Path Variables</title>
    <xref type="rfc" data="rfc8881"/>
    <expert>Unassigned</expert>
    <range>
      <value>ietf.org domain</value>
      <registration_rule>Standards Action with Expert Review</registration_rule>
    </range>
    <range>
      <value>non-ietf.org. domain</value>
      <registration_rule>First Come First Served</registration_rule>
    </range>
    <record>
      <name>${ietf.org:CPU_ARCH}</name>
      <xref type="rfc" data="rfc8881"/>
      <contact><xref type="person" data="IESG"/></contact>
    </record>
    <record>
      <name>${ietf.org:OS_TYPE}</name>
      <xref type="rfc" data="rfc8881"/>
      <contact><xref type="person" data="IESG"/></contact>
    </record>
    <record>
      <name>${ietf.org:OS_VERSION}</name>
      <xref type="rfc" data="rfc8881"/>
      <contact><xref type="person" data="IESG"/></contact>
    </record>
  </registry>
  
  <registry id="nfsv4-path-variables-2">
    <title>NFSv4 ${ietf.org:CPU_ARCH} Values</title>
    <xref type="rfc" data="rfc8881"/>
    <registration_rule>First Come First Served</registration_rule>
  </registry>
  
  <registry id="nfsv4-path-variables-3">
    <title>NFSv4 ${ietf.org:OS_TYPE} Values</title>
    <xref type="rfc" data="rfc8881"/>
    <registration_rule>First Come First Served</registration_rule>
  </registry>

  <people>
    
    <person id="IESG">
      <name>IESG</name>
      <uri>mailto:iesg&amp;ietf.org</uri>
      <updated>2009-01-16</updated>
    </person>
    
  </people>

</registry>

<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="apex.xsl"?>
<?xml-model href="apex.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="apex">
  <title>APEX Parameters</title>
  <updated>2009-06-03</updated>
  
  <registry id="apex-1">
    <title>Applicable Exchange Core (APEX) Endpoint Applications</title>
    <xref type="rfc" data="rfc3340"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
  </registry>
  
  <registry id="apex-2">
    <title>Applicable Exchange Core (APEX) Options</title>
    <xref type="rfc" data="rfc3340"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <option>attachOverride</option>
      <xref type="rfc" data="rfc3342"/>
    </record>
    
    <record>
      <option>dataHopping</option>
      <xref type="rfc" data="rfc3342"/>
    </record>
    <record>
      <option>dataTiming</option>
      <xref type="rfc" data="rfc3342"/>
    </record>
    <record>
      <option>hold4Endpoint</option>
      <xref type="rfc" data="rfc3342"/>
    </record>
    <record>
      <option>statusRequest</option>
      <xref type="rfc" data="rfc3340"/>
    </record>
  </registry>

  <registry id="apex-3">
    <title>Applicable Exchange Core (APEX) Services</title>
    <xref type="rfc" data="rfc3340"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Unassigned</expert>
    <note>The endpoints "apex=all" and "apex=core" are reserved and may not be assigned.
    </note>
    <record>
      <service>apex=access</service>
      <xref type="rfc" data="rfc3341"/>
    </record>
    <record>
      <service>apex=presence</service>
      <xref type="rfc" data="rfc3343"/>
    </record>
    <record>
      <service>apex=report</service>
      <xref type="rfc" data="rfc3340"/>
    </record>
  </registry>
  <people/>
</registry>

<?xml-model href="alto-protocol.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="alto-protocol">
  <title>Application-Layer Traffic Optimization (ALTO) Protocol</title>
  <created>2014-03-18</created>
  <updated>2022-03-29</updated>
  
  <registry id="cost-metrics">
    <title>ALTO Cost Metrics</title>
    <xref type="rfc" data="rfc7285"/>
    <registration_rule>IETF Review</registration_rule>
    <record>
      <value>routingcost</value>
      <description>See Section 6.1.1.1 of <xref type="rfc" data="rfc7285"/></description>
      <xref type="rfc" data="rfc7285"/>
    </record>    
    <record>
      <value>priv:</value>
      <description>Private use</description>
      <xref type="rfc" data="rfc7285"/>
    </record> 
    <record date="2022-03-28">
      <value>delay-ow</value>
      <description>See Section 4.1 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record> 
    <record date="2022-03-28">
      <value>delay-rt</value>
      <description>See Section 4.2 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>delay-variation</value>
      <description>See Section 4.3 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>lossrate</value>
      <description>See Section 4.4 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>hopcount</value>
      <description>See Section 4.5 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>tput</value>
      <description>See Section 5.1 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>bw-residual</value>
      <description>See Section 5.2 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>bw-available</value>
      <description>See Section 5.3 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-29">
      <value>ane-path</value>
      <description>See Section 6.5.1 of <xref type="draft" data="RFC-ietf-alto-path-vector-25"/></description>
      <xref type="draft" data="RFC-ietf-alto-path-vector-25"/>
    </record>
  </registry>
  
  <registry id="cost-source">
    <title>ALTO Cost Source</title>
    <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    <registration_rule>IETF Review</registration_rule>
    <record date="2022-03-28">
      <value>nominal</value>
      <description>Values in nominal cases; Section 3.1 of <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>    
    <record date="2022-03-28">
      <value>sla</value>
      <description>Values reflecting service level agreement; Section 3.1 <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
    <record date="2022-03-28">
      <value>estimation</value>
      <description>Values by estimation; Section 3.1 <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/></description>
      <xref type="draft" data="RFC-ietf-alto-performance-metrics-28"/>
    </record>
  </registry>
  
  <registry id="endpoint-property-types">
    <title>ALTO Endpoint Property Types</title>
    <xref type="rfc" data="rfc7285"/>
    <registration_rule>IETF Review</registration_rule>
    <record>
      <value>pid</value>
      <description>See Section 7.1.1 of <xref type="rfc" data="rfc7285"/></description>
      <xref type="rfc" data="rfc7285"/>

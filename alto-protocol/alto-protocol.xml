    <record date="2022-03-09">
      <value>asn</value>
      <entity>See <xref type="draft" data="RFC-ietf-alto-cdni-request-routing-alto-22"/>, Section 6.1.1.2</entity>
      <hier-inher>None</hier-inher>
      <mediatype>None</mediatype>
      <mapping>false</mapping>
      <xref type="draft" data="RFC-ietf-alto-cdni-request-routing-alto-22"/>
    </record>
    <record date="2022-03-09">
      <value>countrycode</value>
      <entity>See <xref type="draft" data="RFC-ietf-alto-cdni-request-routing-alto-22"/>, Section 6.1.2.2</entity>
      <hier-inher>None</hier-inher>
      <mediatype>None</mediatype>
      <mapping>false</mapping>
      <xref type="draft" data="RFC-ietf-alto-cdni-request-routing-alto-22"/>
    </record>
    <record date="2022-03-29">
      <value>ane</value>

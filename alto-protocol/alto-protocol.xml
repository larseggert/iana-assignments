      <value>ipv4</value>
      <entity>See <xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>, Section 6.1.1</entity>
      <hier-inher>See <xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>, Section 6.1.3</hier-inher>
      <mediatype>application/alto-networkmap+json</mediatype>
      <mapping>true</mapping>
      <xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>
    </record>
    <record date="2022-02-28">
      <value>ipv6</value>
      <entity>See <xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>, Section 6.1.2</entity>
      <hier-inher><xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>, Section 6.1.3</hier-inher>
      <mediatype>application/alto-networkmap+json</mediatype>
      <mapping>true</mapping>
      <xref type="draft" data="RFC-ietf-alto-unified-props-new-24"/>
    </record>
    <record date="2022-02-28">
      <value>pid</value>

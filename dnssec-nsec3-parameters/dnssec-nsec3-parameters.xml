<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="dnssec-nsec3-parameters.xsl"?>
<?xml-model href="dnssec-nsec3-parameters.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="dnssec-nsec3-parameters">
  <title>Domain Name System Security (DNSSEC) NextSECure3 (NSEC3) Parameters</title>
  <created>2007-12-17</created>
  <updated>2008-03-05</updated>
  <registry id="dnssec-nsec3-parameters-1">
    <title>DNSSEC NSEC3 Flags</title>
    <xref type="rfc" data="rfc5155"/>
    <registration_rule>Standards Action</registration_rule>
    <record>
      <value>0-6</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>7</value>
      <description>Opt-Out</description>
      <xref type="rfc" data="rfc5155"/>
    </record>
  </registry>
  <registry id="dnssec-nsec3-parameters-2">
    <title>DNSSEC NSEC3PARAM Flags</title>
    <xref type="rfc" data="rfc5155"/>
    <registration_rule>Standards Action</registration_rule>
    <record>
      <value>0-6</value>
      <description>Unassigned</description>
    </record>
    <record>
      <value>7</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc5155"/>
    </record>
  </registry>
  <registry id="dnssec-nsec3-parameters-3">
    <title>DNSSEC NSEC3 Hash Algorithms</title>
    <xref type="rfc" data="rfc5155"/>
    <registration_rule>Standards Action</registration_rule>
    <record>
      <value>0</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc5155"/>
    </record>
    <record>
      <value>1</value>
      <description>SHA-1</description>
      <xref type="rfc" data="rfc5155"/>
    </record>
    <record>
      <value>2-255</value>
      <description>Unassigned</description>
    </record>
  </registry>
  <people/>
</registry>

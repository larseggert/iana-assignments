<registry xmlns="http://www.iana.org/assignments" id="pkix-parameters">
  <title>Public Key Infrastructure using X.509 (PKIX) Parameters</title>
  <created>2003-12-23</created>
  <updated>2023-08-17</updated>
  
  <registry id="srv-protocol-labels">
    <title>PKIX SRV Protocol Labels</title>
    <xref type="rfc" data="rfc4386"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <name>_PKIXREP</name>
      <xref type="rfc" data="rfc4386"/>
    </record>
  </registry>
  
  <registry id="caa-properties">

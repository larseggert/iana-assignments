  <registry id="caa-flags">
    <title>Certification Authority Restriction Flags</title>
    <xref type="rfc" data="rfc8659"/>
    <registration_rule>RFC Required</registration_rule>
    <record>
      <value>0</value>
      <description>Issuer Critical Flag</description>
      <xref type="rfc" data="rfc8659"/>
    </record>
    <record>
      <value>1-7</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc8659"/>
    </record>

    <record>
      <name>issue</name>
      <description>Authorization Entry by Domain</description>
      <xref type="rfc" data="rfc8659"/>
    </record>
    <record>
      <name>issuewild</name>
      <description>Authorization Entry by Wildcard Domain</description>
      <xref type="rfc" data="rfc8659"/>
    </record>
    <record>
      <name>iodef</name>
      <description>Report incident by IODEF report</description>
      <xref type="rfc" data="rfc8659"/>
    </record>
    <record>
      <name>auth</name>
      <description>Reserved</description>
      <xref type="rfc" data="rfc8659"/>
    </record>
    <record>
      <name>path</name>
      <description>Reserved</description>
      <xref type="rfc" data="rfc8659"/>

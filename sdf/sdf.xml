      <reference><xref type="draft" data="RFC-ietf-asdf-sdf-23"/></reference>
      <controller>IETF</controller>
    </record>
    <record date="2025-03-27">
      <value>readable</value>
      <description>boolean: can the item be read?</description>
      <reference><xref type="draft" data="RFC-ietf-asdf-sdf-23"/></reference>
      <controller>IETF</controller>
    </record>

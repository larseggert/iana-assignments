      <reference><xref type="draft" data="RFC-ietf-asdf-sdf-23"/></reference>
      <controller>IETF</controller>
    </record>
    <record date="2025-03-27">
      <value>pattern</value>
      <description>regular expression pattern for a text string</description>
      <reference><xref type="draft" data="RFC-ietf-asdf-sdf-23"/></reference>
      <controller>IETF</controller>
    </record>
    <record date="2025-03-27">
      <value>properties</value>

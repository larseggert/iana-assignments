  <registry id="framing-header-bits">
    <title>WebSocket Framing Header Bits Registry</title>
    <registration_rule>Standards Action</registration_rule>
    <xref type="rfc" data="rfc6455"/>
    <record date="2015-09-02">
      <value>RSV1</value>

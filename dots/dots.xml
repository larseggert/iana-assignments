    </note>
    <record date="2020-01-16">
      <value>0</value>
      <name>Reserved</name>
      <description/>
      <xref type="rfc" data="rfc9132"/>
    </record>
    <record date="2020-01-16">
      <value>1</value>
      <name>request-inactive-other-active</name>
      <description>DOTS server has detected conflicting mitigation 
        requests from different DOTS clients. This mitigation request is 
        currently inactive until the conflicts are resolved. 
        Another mitigation request is active.</description>
        <xref type="rfc" data="rfc9132"/>
    </record>
    <record date="2020-01-16">
      <value>2</value>
      <name>request-active</name>
      <description>DOTS server has detected conflicting mitigation 
        requests from different DOTS clients. This mitigation 
        request is currently active.</description>
        <xref type="rfc" data="rfc9132"/>
    </record>
    <record date="2020-01-16">
      <value>3</value>
      <name>all-requests-inactive</name>
      <description>DOTS server has detected conflicting mitigation 
        requests from different DOTS clients. All conflicting 
        mitigation requests are inactive.</description>
        <xref type="rfc" data="rfc9132"/>

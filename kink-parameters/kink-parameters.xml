  <registry id="kink-parameters-6">
    <title>KINK Error Codes</title>
    <xref type="rfc" data="rfc4430"/>
    <expert>Stewart Bryant, Danny McPherson</expert>
    <range>
      <value>0-8191</value>
      <registration_rule>Standards Action or non standards-track   
RFCs with Expert Review</registration_rule>
      <note>Though full specification may be a public and
permanent document of a standards body other that
the IETF, an RFC referring to it is needed.</note>
    </range>
    <range>
      <value>8192-16383</value>
      <registration_rule>Private Use</registration_rule>
    </range>
    <range>
      <value>16384-4294967295</value>
      <registration_rule>Reserved</registration_rule>
    </range>
    <record>
      <value>0</value>
      <description>KINK_OK</description>
      <purpose>No error detected</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>1</value>
      <description>KINK_PROTOERR</description>
      <purpose>The message was malformed</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>2</value>
      <description>KINK_INVDOI</description>
      <purpose>Invalid DOI</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>3</value>
      <description>KINK_INVMAJ</description>
      <purpose>Invalid Major Version</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>4</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>5</value>
      <description>KINK_INTERR</description>
      <purpose>An unrevoverable internal error</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>6</value>
      <description>KINK_BADQMVERS</description>
      <purpose>Unsupported Quick Mode Version</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>7</value>
      <description>KINK_U2UDENIED</description>
      <purpose>Returning a TGT is prohibited</purpose>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>8-8191</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>8192-16383</value>
      <description>Private use</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>16384-4294967295</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc4430"/>
    </record>

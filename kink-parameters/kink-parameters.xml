  <registry id="kink-parameters-2">
    <title>KINK Message Types</title>
    <xref type="rfc" data="rfc4430"/>
    <expert>Shoichi Sakane, Ken'ichi Kamada</expert>
    <range>
      <value>1-127</value>
      <registration_rule>Standards Action or non standards-track   
RFCs with Expert Review</registration_rule>
      <note>Though full specification may be a public and
permanent document of a standards body other that
the IETF, an RFC referring to it is needed.</note>
    </range>
    <range>
      <value>128-255</value>
      <registration_rule>Private Use</registration_rule>
    </range>
    <record>
      <value>0</value>
      <description>RESERVED</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>1</value>
      <description>CREATE</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>2</value>
      <description>DELETE</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>3</value>
      <description>REPLY</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>4</value>
      <description>GETTGT</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>5</value>
      <description>ACK</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>6</value>
      <description>STATUS</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>7-127</value>
      <description>Unassigned</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
    <record>
      <value>128-255</value>
      <description>Private use</description>
      <xref type="rfc" data="rfc4430"/>
    </record>
  </registry>


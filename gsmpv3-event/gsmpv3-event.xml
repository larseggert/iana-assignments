<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="gsmpv3-event.xsl"?>
<?xml-model href="gsmpv3-event.rng" schematypens="http://relaxng.org/ns/structure/1.0" ?>
<registry xmlns="http://www.iana.org/assignments" id="gsmpv3-event">
  <title>Global Switch Management Protocol version 3 (GSMPv3) Event Flag Name Space</title>
  <created>2002-03-28</created>
  <updated>2023-08-25</updated>
  <registry id="gsmpv3-event-1">
    <title>Event Flags</title>
    <xref type="rfc" data="rfc3292"/>
    <registration_rule>Expert Review</registration_rule>
    <expert>Stefaan De Cnodder, Avri Doria</expert>
    <note>The most significant bit is 0.</note>
    <record>
      <bit>0</bit>
      <flag>U: Port Up</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>1</bit>
      <flag>D: Port Down</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>2</bit>
      <flag>I: Invalid Label</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>3</bit>
      <flag>N: New Port</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>4</bit>
      <flag>Z: Dead Port</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>5</bit>
      <flag>A: Adjacency Event</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
    <record>
      <bit>6-15</bit>
      <flag>x: Unused</flag>
      <xref type="rfc" data="rfc3292"/>
    </record>
  </registry>
  <people/>
</registry>

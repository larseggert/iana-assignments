        </record>
        <record>
            <value>1-7</value>
            <name>Unassigned</name>
        </record>
    </registry>
    
    <registry id="bundle-offer-response-b-flags">
        <title>Bundle Offer and Response B Flags</title>
        <xref type="rfc" data="rfc6693"/>
        <registration_rule>Specification Required</registration_rule>
        <record>
            <value>0</value>

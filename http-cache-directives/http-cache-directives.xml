<?oxygen RNGSchema="http-cache-directives.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="http-cache-directives">
  <title>Hypertext Transfer Protocol (HTTP) Cache Directive Registry</title>
  <created>2014-02-17</created>
  <updated>2017-08-07</updated>
  
  <registry id="cache-directives">
    <title>HTTP Cache Directive Registry</title>
    <xref type="rfc" data="rfc7234"/>
    <registration_rule>IETF Review</registration_rule>
    <record date="2017-08-07">
      <value>immutable</value>
      <xref type="draft" data="RFC-ietf-httpbis-immutable-03"/>
    </record>
    <record>
      <value>max-age</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.1, 5.2.2.8</xref>
    </record>
    <record>
      <value>max-stale</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.2</xref>
    </record>
    <record>
      <value>min-fresh</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.3</xref>
    </record>
    <record>
      <value>must-revalidate</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.2.1</xref>
    </record>
    <record>
      <value>no-cache</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.4, 5.2.2.2</xref>
    </record>
    <record>
      <value>no-store</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.5, 5.2.2.3</xref>
    </record>
    <record>
      <value>no-transform</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.6, 5.2.2.4</xref>
    </record>
    <record>
      <value>only-if-cached</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.1.7</xref>
    </record>
    <record>
      <value>private</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.2.6</xref>
    </record>
    <record>
      <value>proxy-revalidate</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.2.7</xref>
    </record>
    <record>
      <value>public</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.2.5</xref>
    </record>
    <record>
      <value>s-maxage</value>
      <xref type="rfc" data="rfc7234">RFC7234, Section 5.2.2.9</xref>
    </record>
    <record>
      <value>stale-if-error</value>
      <xref type="rfc" data="rfc5861">RFC5861, Section 4</xref>
    </record>
    <record>
      <value>stale-while-revalidate</value>
      <xref type="rfc" data="rfc5861">RFC5861, Section 3</xref>
    </record>
  </registry>
  
  <people/>
</registry>

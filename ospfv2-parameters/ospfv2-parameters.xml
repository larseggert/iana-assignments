    </record>
    <record>
      <value>1</value>
      <description>Hello</description>
      <xref type="rfc" data="rfc2328"/>
    </record>
    <record>
      <value>2</value>
      <description>Database Description</description>
      <xref type="rfc" data="rfc2328"/>
    </record>
    <record>
      <value>3</value>
      <description>Link State Request</description>
      <xref type="rfc" data="rfc2328"/>
    </record>
    <record>
      <value>4</value>
      <description>Link State Update</description>
      <xref type="rfc" data="rfc2328"/>
    </record>
    <record>
      <value>5</value>

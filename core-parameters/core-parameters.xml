  <registry id="codes">
    <title>CoAP Codes</title>
    <xref type="rfc" data="rfc7252">RFC7252, Section 12.1</xref>
    <registration_rule>Reserved ranges may be allocated in accordance with section 4.3 of <xref type="rfc" data="rfc5226"/>.</registration_rule>
    <record>
      <value>0.00</value>
      <description>Indicates an Empty message.</description>
      <xref type="rfc" data="rfc7252">RFC7252, section 4.1</xref>
    </record>    
    <record>
      <value>0.01-0.31</value>
      <description>Indicates a request.  Values in this range are assigned by the 
      <xref type="uri" data="http://www.iana.org/assignments/core-parameters/core-parameters.xhtml#method-codes">CoAP Method Codes</xref> sub-registry.</description>
      <xref type="rfc" data="rfc7252">RFC7252, section 12.1.1</xref>
    </record>    
    <record>
      <value>1.00-1.31</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc7252"/>
    </record>    
    <record>
      <value>2.00-5.31</value>
      <description>Indicates a response.  Values in this range are assigned by the 
      <xref type="uri" data="http://www.iana.org/assignments/core-parameters/core-parameters.xhtml#response-codes">CoAP Response Codes</xref> sub-registry.</description>
      <xref type="rfc" data="rfc7252">RFC7252, section 12.1.2</xref>
    </record>
    <record>
      <value>6.00-7.31</value>
      <description>Reserved</description>
      <xref type="rfc" data="rfc7252"/>
    </record>
    
    
